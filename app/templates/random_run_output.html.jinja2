{% for unit in units %}
{% if loop.first %}
<h2>{{unit.name}} Route</h2>
<h4>Units</h4>
<div class="generator-grid">
    {% endif %}
    <article>
        <header><strong>{{unit.name}}</strong></header>
        <div>
            <img src="/static/images/portraits/{{ unit.portrait_nid|urlencode }}.png" height="96" height="80" />
            <div>
                {% for class in output_map[unit.nid] %}
                <img src="{{ url_for('static',filename='images/map_sprites/'+class.map_sprite_nid+'-stand-static.webp') }}"
                    height="48" width="48" />
                {{ class.name }} {% if class.weapons%}
                {% for weapon in class.weapons%}
                <span class="{{ weapon.icon_class }}"></span>
